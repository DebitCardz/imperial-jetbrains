<idea-plugin>
    <id>in.imperialb.imperialjetbrains</id>
    <name>IMPERIAL</name>
    <vendor email="noreply@imperialb.in" url="https://imperialb.in">IMPERIAL</vendor>

    <description>JetBrains extension to easily upload documents to IMPERIAL</description>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <group id="IMPERIAL.CreateDocument" text="Create Document" description="Create the document" >
            <add-to-group group-id="CutCopyPasteGroup" anchor="first" />
            <action class="in.imperialb.imperialjetbrains.CreateDocument" id="IMPERIAL.Actions.CreateDocument" text="Create Document" />
        </group>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>